    "keywords": ["lariele", "form", "laravel", "livewire"],
    "license": "MIT",
    "authors": [
        {
            "name": "Daniel Cepec",
            "email": "daniel.cepec@gmail.com"
        }
    ],
    "require": {
        "php": "^7.4|^8.0"
    },
    "extra": {
        "laravel": {
